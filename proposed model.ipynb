    "\n",

    "file_path=['violin']\n",
    "\n",
    "all_files=glob.glob('All Midi Files/'+file_path[0]+'/*.mid',recursive=True)\n",
    "\n",

    "notes_array = np.array([read_files(i) for i in tqdm(all_files,position=0,leave=True)])\n"
   ]
