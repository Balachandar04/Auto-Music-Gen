   "source": [
    "print(notes_array.shape)\n",

    "if notes_array.shape[0]!=1:\n",
    "    notess = sum(notes_array,[])\n",
    "else: \n",
    "    notess = notes_array[0].tolist()\n",
    "unique_notes = list(set(notess))\n",
    "print(\"Unique Notes:\",len(unique_notes))\n"
   ]
  },
