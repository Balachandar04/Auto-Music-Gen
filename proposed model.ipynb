    "def generate_music(model, x_test, ind2note, output_file, instrument_type):\n",
    "    \"\"\"\n",
    "    Generate music using the given model and save it to a MIDI file.\n",
    "\n",
    "    Parameters:\n",
    "    model (keras.models.Model): Trained model to generate music\n",
    "    x_test (numpy.ndarray): Test dataset used to seed the model\n",
    "    ind2note (dict): Mapping of index to note values\n",
    "    output_file (str): Path to save the generated music as a MIDI file\n",
    "    instrument_type (str): Type of instrument to use for the output music\n",
    "\n",
    "    Returns:\n",
    "    None\n",
    "    \"\"\"\n",
    "    # Generate random index\n",
    "    index = np.random.randint(0, len(x_test)-1)\n",
    "    print(index)\n",
    "    \n",
    "    # Get the data of generated index from x_test\n",
    "    music_pattern = x_test[index]\n",
    "\n",
    "    out_pred=[] # It will store predicted notes\n",
    "\n",
    "    # Iterate until 200 notes are generated\n",
    "    for i in range(100):\n",
    "\n",
    "        # Reshape the music pattern \n",
    "        music_pattern = music_pattern.reshape(1, len(music_pattern), 1)\n",
    "\n",
    "        # Get the maximum probability value from the predicted output\n",
    "        pred_index = np.argmax(model.predict(music_pattern))\n",
    "\n",
    "        # Get the note using predicted index and append to the output prediction list\n",
    "        out_pred.append(ind2note[pred_index])\n",
    "\n",
    "        # Append the predicted index to the music pattern and update it with one timestep ahead\n",
    "        music_pattern = np.append(music_pattern, pred_index)\n",
    "        music_pattern = music_pattern[1:]\n",
    "\n",
    "    output_notes = []\n",
    "\n",
    "    for offset, pattern in enumerate(out_pred):\n",
    "\n",
    "        # If pattern is a chord instance\n",
    "        if ('.' in pattern) or pattern.isdigit():\n",
    "\n",
    "            # Split notes from the chord\n",
    "            notes_in_chord = pattern.split('.')\n",
    "            notes = []\n",
    "\n",
    "            for current_note in notes_in_chord:\n",
    "                i_curr_note = int(current_note)\n",
    "\n",
    "                # Cast the current note to Note object and append the current note\n",
    "                new_note = note.Note()\n",
    "                new_note.pitch.midi = i_curr_note\n",
    "                new_note.storedInstrument = instrument.fromString(instrument_type)\n",
    "                notes.append(new_note)\n",
    "\n",
    "            # Cast the current note to Chord object, and append it\n",
    "            # Offset will be 1 step ahead from the previous note\n",
    "            # as it will prevent notes from stacking up \n",
    "            new_chord = chord.Chord(notes)\n",
    "            new_chord.offset = offset\n",
    "            output_notes.append(new_chord)\n",
    "\n",
    "        else:\n",
    "            # Cast the pattern to Note object, apply the offset, and append the note\n",
    "            g4 = note.Note(pattern)\n",
    "            midi_pitch = g4.pitch.midi\n",
    "            new_note = note.Note()\n",
    "            new_note.pitch.midi = midi_pitch\n",
    "            new_note.offset = offset\n",
    "            new_note.storedInstrument = instrument.fromString(instrument_type)\n",
    "            output_notes.append(new_note)\n",
    "\n",
    "    # Create a stream object from the generated output notes\n",
    "    midi_stream = stream.Stream(output_notes)\n",
    "\n",
    "    # Write the MIDI file\n",
    "    midi_stream.write('midi', fp=output_file)\n"
   ]
