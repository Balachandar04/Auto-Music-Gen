   "source": [
    "\n",

    "freq=dict(map(lambda x: (x,notess.count(x)),unique_notes))\n",
    "\n",

    "print(\"\\nFrequency notes\")\n",
    "for i in range(30,100,20):\n",
    "  print(i,\":\",len(list(filter(lambda x:x[1]>=i,freq.items()))))\n"
   ]
  },
