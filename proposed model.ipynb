    "import numpy as np\n",
    "from music21 import converter, instrument, note, chord, stream\n",
    "\n",
    "def generate_music(model, x_test, ind2note, length=100, instrument_name=\"Piano\"):\n",

    "    index = np.random.randint(0,len(x_test)-1)\n",
    "    print(\"Generating music using index\", index)\n",

    "    music_pattern = x_test[index]\n",
    "\n",
    "    out_pred=[] # it will store predicted notes\n",
    "\n",

    "    for i in range(length):\n",
    "\n",

    "        music_pattern = music_pattern.reshape(1,len(music_pattern),1)\n",
    "\n",

    "        pred_index = np.argmax(model.predict(music_pattern))\n",
    "\n",

    "        out_pred.append(ind2note[pred_index])\n",
    "\n",

    "        music_pattern = np.append(music_pattern,pred_index)\n",
    "\n",

    "        music_pattern = music_pattern[1:]\n",
    "\n",

    "    output_stream = stream.Stream()\n",
    "\n",

    "    for offset, pattern in enumerate(out_pred):\n",
    "\n",

    "        if ('.' in pattern) or pattern.isdigit():\n",
    "\n",

    "            notes_in_chord = pattern.split('.')\n",
    "            notes = []\n",
    "\n",
    "            for current_note in notes_in_chord:\n",
    "                i_curr_note=int(current_note)\n",
    "             
    "                new_note = note.Note(i_curr_note)\n",
    "                notes.append(new_note)\n",
    "\n",

    "            new_chord = chord.Chord(notes)\n",
    "            new_chord.offset = offset\n",
    "            output_stream.append(new_chord)\n",
    "\n",
    "        else:\n",,
    "            new_note = note.Note(pattern)\n",
    "            new_note.offset = offset\n",
    "            output_stream.append(new_note)\n",
    "\n",

    "    try:\n",
    "        instrument_obj = instrument.instrumentFromName(instrument_name)\n",
    "    except:\n",
    "        print(f\"Instrument '{instrument_name}' not found. Defaulting to piano.\")\n",
    "        instrument_obj = instrument.Piano()\n",
    "    output_stream.insert(0, instrument_obj)\n",
    "\n",

    "    midi_file = converter.parse\n",
    "    midi_file.insert(0, output_stream)\n",
    "    midi_file.write(\"output.mid\")\n",
    "    print(\"MIDI file saved as 'output.mid'\")\n"
